{
    "hostRequirements": {
        "cpus": 4
    },
    "waitFor": "onCreateCommand",
    "updateContentCommand": "yarn install && yarn run build",
    "postCreateCommand": "",
    "postAttachCommand": {
        "server": "yarn run dev"
    },
    "customizations": {
        "codespaces": {
            "openFiles": [
                "frontend/pages/index.js",
            ]
        }
    },
    "portsAttributes": {
        "1337": {
            "label": "Strapi",
            "onAutoForward": "openPreview"
        },
        "3000": {
            "label": "Application",
            "onAutoForward": "openPreview"
        }
    },
    "forwardPorts": [1337, 3000]
}
